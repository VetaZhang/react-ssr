{
  "name": "ssr",
  "version": "0.0.1",
  "description": "react ssr",
  "main": "server/server.js",
  "scripts": {
    "dev": "npm run clean && npm run devServer",
    "clean": "rimraf ./dist",
    "devServer": "NODE_ENV=development node ./server/index.js",
    "daily": "npm run clean && npm run build:daily-client && npm run build:daily-server && npm run setup:daily-server",
    "build:daily-client": "NODE_ENV=daily webpack --config ./webpack/client.daily.js",
    "build:daily-server": "NODE_ENV=daily webpack --config ./webpack/server.js",
    "setup:daily-server": "NODE_ENV=daily node ./server/index.js",
    "prod": "npm run clean && npm run build:prod-client && npm run build:prod-server && npm run setup:prod-server",
    "build:prod-client": "NODE_ENV=production webpack --config ./webpack/client.prod.js",
    "build:prod-server": "NODE_ENV=production webpack --config ./webpack/server.js",
    "setup:prod-server": "NODE_ENV=production node ./server/index.js"
  },
  "keywords": [
    "react",
    "ssr"
  ],
  "author": "Veta",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.10.1",
    "@loadable/server": "^5.9.0",
    "cookie-parser": "^1.4.4",
    "ejs": "^2.6.2",
    "express": "^4.16.4",
    "lru-cache": "^5.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "styled-components": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "4.19.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.13.0",
    "happypack": "^5.0.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "postcss-extend": "^1.0.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "precss": "^4.0.0",
    "qiniu-webpack-plugin": "^0.4.2",
    "react-hot-loader": "^4.12.3",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
